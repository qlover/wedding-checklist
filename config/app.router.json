{
  "base": {
    "routes": [
      {
        "path": "/",
        "element": "base/RedirectPathname",
        "meta": {
          "category": "main"
        }
      },
      {
        "path": "/:lng",
        "element": "base/Layout",
        "meta": {
          "category": "main"
        },
        "children": [
          {
            "index": true,
            "element": "base/checklist/ChecklistPage",
            "meta": {
              "title": "page.home.title",
              "icon": "home",
              "localNamespace": "common"
            }
          },
          {
            "path": "about",
            "element": "base/About",
            "meta": {
              "title": "page.about.title",
              "icon": "info",
              "localNamespace": "common"
            }
          },
          {
            "path": "404",
            "element": "404",
            "meta": {
              "category": "common",
              "title": "page.404.title",
              "layout": "blank",
              "localNamespace": "common"
            }
          },
          {
            "path": "500",
            "element": "500",
            "meta": {
              "category": "common",
              "title": "page.500.title",
              "layout": "blank",
              "localNamespace": "common"
            }
          }
        ]
      },

      {
        "path": "/:lng",
        "element": "auth/Layout",
        "meta": {
          "category": "auth"
        },
        "children": [
          {
            "index": true,
            "element": "auth/Login"
          },
          {
            "path": "login",
            "element": "auth/Login",
            "meta": {
              "title": "page.login.title",
              "icon": "info",
              "localNamespace": "common"
            }
          }
        ]
      },

      {
        "path": "404",
        "element": "404",
        "meta": {
          "category": "common",
          "title": "page.404.title",
          "layout": "blank",
          "localNamespace": "common"
        }
      },
      {
        "path": "500",
        "element": "500",
        "meta": {
          "category": "common",
          "title": "page.500.title",
          "layout": "blank",
          "localNamespace": "common"
        }
      },
      {
        "path": "*",
        "element": "404",
        "meta": {
          "category": "common",
          "title": "page.404.title",
          "layout": "blank",
          "localNamespace": "common"
        }
      }
    ]
  }
}
